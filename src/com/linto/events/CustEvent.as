package com.linto.events{	import flash.events.Event;		public class CustEvent extends Event{		public var data:Object;					public function CustEvent(type:String,bubbles:Boolean=false, cancelable:Boolean=false){			super(type, bubbles, cancelable);		}				// Every custom event class must override clone( )		public override function clone( ):Event {			return new CustEvent(type, bubbles, cancelable);		}		// Every custom event class must override toString( ). Note that		// "eventPhase" is an instance variable relating to the event flow.		// See Chapter 21.		public override function toString( ):String {			return formatToString("CustEvent", "type", "bubbles",				"cancelable", "eventPhase");		}	}}